				{
					"name": "3.6 Create JWT IDP for seeding groups",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"\r",
									"eval( pm.collectionVariables.get('pmlib_code') );\r",
									"var jwtSecret = pm.collectionVariables.get('private_key');\r",
									"\r",
									"var payload = {\r",
									"  \"sub\": \"tmpgroupprovisioning\",\r",
									"  \"subType\": \"user\",\r",
									"  \"name\": \"temporary group user\",\r",
									"  \"email\": \"noreply@qlik.com\",\r",
									"  \"email_verified\": false,\r",
									"  \"groups\": pm.collectionVariables.get('group'),\r",
									"  \"aud\": \"qlik.api/login/jwt-session\",\r",
									"  \"iss\": pm.collectionVariables.get('issuer')\r",
									"}\r",
									"\r",
									"var alg ='RS256';\r",
									"var kid = pm.collectionVariables.get('key_id');\r",
									"\r",
									"var jwt_token = pmlib.createJws(jwtSecret, payload, {}, exp=600, alg, kid);\r",
									"\r",
									"pm.collectionVariables.set('jwt_token', jwt_token);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
