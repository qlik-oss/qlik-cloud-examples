# Qlik Cloud Host (e.g., your-tenant.us.qlikcloud.com)
QLIK_HOST=your-tenant.region.qlikcloud.com

# OAuth Client ID from your OAuth client configuration
QLIK_CLIENT_ID=your_client_id_here

# OAuth Client Secret from your OAuth client configuration
QLIK_CLIENT_SECRET=your_client_secret_here

# Qlik Sense app ID (e.g., 0ef9c2e0-a9d7-4a54-912b-1d982effdf38)
QLIK_APP_ID=your_app_id

# Save object layouts to disk (default: false)
# Set to 'true' to save all object layouts as JSON files in the layouts/ directory
SAVE_LAYOUTS=false